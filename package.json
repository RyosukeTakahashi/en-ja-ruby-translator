{
  "name": "en-ja-ruby-translator",
  "description": "english to japanese word translator",
  "version": "1.0.0",
  "homepage": "https://github.com/azu/en-ja-ruby-translator/",
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/en-ja-ruby-translator.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "browserify -t brfs index.js -o dist/en-ja-ruby-translator.user.js",
    "watch": "watchify -t brfs index.js -o `git config greasemonkey.file || echo en-ja-ruby-translator.user.js`",
    "test": "mocha test/*.js"
  },
  "directories": {
    "test": "test/"
  },
  "author": "azu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/azu/en-ja-ruby-translator/issues"
  },
  "devDependencies": {
    "brfs": "^1.2.0",
    "browserify": "^6.3.3",
    "espower-loader": "^0.10.0",
    "intelli-espower-loader": "^0.5.0",
    "jsdom": "^1.3.1",
    "mocha": "^2.0.1",
    "mocha-jsdom": "^0.2.0",
    "power-assert": "^0.10.0",
    "watchify": "^2.1.1",
    "x-readline": "^1.0.1"
  }
}
